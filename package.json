{
  "name": "@guardian/grid-cli",
  "description": "Helpful commands for interacting with Grid, The Guardian's image management tool.",
  "version": "1.4.0",
  "author": "The Guardian",
  "bin": {
    "grid": "bin/run"
  },
  "bugs": "https://github.com/guardian/grid-cli/issues",
  "dependencies": {
    "@oclif/core": "^1.16.0",
    "@oclif/plugin-autocomplete": "^1.3.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "@types/aws-sdk": "^2.7.0",
    "@types/diff": "^4.0.2",
    "colors": "^1.4.0",
    "diff": "^4.0.2",
    "lines-async-iterator": "^0.9.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.0",
    "open": "^6.4.0",
    "terminal-image": "^1.2.1",
    "terminal-link": "^2.1.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@types/lodash": "^4.14.163",
    "@types/node": "^18.7.14",
    "@types/node-fetch": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^10.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/guardian/grid-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "grid",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "guardian/grid-cli",
  "scripts": {
    "local": "./bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc -f stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
