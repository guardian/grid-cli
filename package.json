{
  "name": "@guardian/grid-cli",
  "description": "Helpful commands for interacting with Grid, The Guardian's image management tool.",
  "version": "1.1.0",
  "author": "The Guardian",
  "bin": {
    "grid": "./bin/run"
  },
  "bugs": "https://github.com/guardian/grid-cli/issues",
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-autocomplete": "^0.1.3",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.3.2",
    "node-fetch": "^2.6.0",
    "open": "^6.4.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/node": "^10.14.17",
    "@types/node-fetch": "^2.5.0",
    "globby": "^10.0.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/guardian/grid-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "grid",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "guardian/grid-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
